{
    "manifest_version": 3,
    "name": "YouTube Downloader",
    "version": "1.0",
    "description": "Download YouTube videos in any quality or audio-only mode.",
    "permissions": ["downloads", "tabs", "scripting"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  